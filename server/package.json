{
  "name": "server",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "dev-old": "cross-env NODE_OPTIONS=\"--loader ts-node/esm\" ts-node-dev --respawn --transpile-only --env-file .env.example src/index.ts",
    "dev": "tsx --env-file=.env.example src/index.ts",
    "dev-watch": "tsx --env-file=.env.example --watch src/index.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.2",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.23",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.11.30",
    "@types/pg": "^8.15.4",
    "@types/socket.io": "^3.0.1",
    "@types/uuid": "^9.0.7",
    "cross-env": "^7.0.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5.4.5"
  }
}
